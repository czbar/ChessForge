<Window x:Class="ChessForge.GamesEvalDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChessForge"
        mc:Ignorable="d"
        Title="GamesEvalDialog" Height="350" Width="400">
    <Grid>
        <ProgressBar x:Name="UiPbProgress" HorizontalAlignment="Center" Height="20" Margin="0,60,0,0" VerticalAlignment="Top" Width="200"/>
        <TextBox x:Name="UiTbTimeRemaining" HorizontalAlignment="Center" Margin="0,160,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Time Remaining" HorizontalAlignment="Center" Margin="0,129,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Cancel" HorizontalAlignment="Center" Margin="0,235,0,0" Width="100" VerticalAlignment="Top"/>
    </Grid>
</Window>
